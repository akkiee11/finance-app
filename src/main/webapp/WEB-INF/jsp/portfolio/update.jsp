<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <title>Update Record</title>    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">    <style>      .wrapper {        width: 600px;        margin: 0 auto;      }    </style>    <%-- TODO: Add validation--%></head><body><div class="wrapper">    <div class="container-fluid">        <div class="row">            <div class="col-md-12">                <h2 class="mt-5">Update Record</h2>                <p>Please edit the input values and submit to update the stock record.</p>                <form action="${pageContext.request.contextPath}/portfolio/update" method="post">                    <div class="form-group">                        <label>Stock Name: ${stock_name}</label>                        <input type="hidden" name="stock_name" class="form-control" value="${stock_name}">                    </div>                    <div class="form-group">                        <label>Price</label>                        <input type="number" name="stock_price" class="form-control" value="${stock_price}">                    </div>                    <div class="form-group">                        <label>Quantity</label>                        <input type="number" name="stock_quantity" class="form-control" value="${stock_quantity}">                    </div>                    <div class="form-group">                        <label>Current Position</label>                        <input type="text" name="stock_position" class="form-control" value="${stock_position}">                    </div>                    <input type="hidden" name="trade_id" value="${trade_id}"/>                    <input type="submit" class="btn btn-primary" value="Submit">                    <input type="button" value="Cancel" class="btn btn-secondary ml-2" onclick="history.back()">                </form>            </div>        </div>    </div></div></body></html>