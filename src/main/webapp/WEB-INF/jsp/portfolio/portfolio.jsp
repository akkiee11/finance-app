<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <title>Dashboard</title>    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>    <style>      .scroll {        overflow: scroll;      }      .wrapper {        width: 600px;        margin: 0 auto;      }      .wrapper-s {        width: 300px;        margin: 0 auto;      }      table tr td:last-child {        width: 120px;      }    </style>    <script>        $(document).ready(function () {            $('[data-toggle="tooltip"]').tooltip();        });    </script></head><body class="scroll"><div class="wrapper" style="position:absolute; left:300px">    <div class="container-fluid">        <div class="row">            <div class="col-md-12">                <div class="mt-5 mb-3 clearfix">                    <h2 class="pull-left">Order History</h2>                    <a href="${pageContext.request.contextPath}/portfolio/create" class="btn btn-success pull-right"><i                            class="fa fa-plus"></i> Add New Stock</a>                </div>                <c:choose>                    <c:when test="${not empty portfolio}">                        <table class="table table-bordered table-striped">                            <thead>                            <tr>                                <th>Stock Name</th>                                <th>Price</th>                                <th>Qty</th>                                <th>Stock Position</th>                                <th>Trade Date</th>                                <th>Action</th>                            </tr>                            </thead>                            <tbody>                            <c:forEach var="stock" items="${portfolio}">                                <tr>                                    <td>${stock.stock}</td>                                    <td>${stock.price}</td>                                    <td>${stock.quantity}</td>                                    <td>${stock.position}</td>                                    <td><fmt:formatDate pattern="dd-MM-yyyy" value="${stock.timestamp}"/></td>                                    <td>                                        <a href="${pageContext.request.contextPath}/portfolio/read/${stock.id}"                                           class="mr-3" title="View Record" data-toggle="tooltip"><span                                                class="fa fa-eye"></span></a>                                        <a href="${pageContext.request.contextPath}/portfolio/update/${stock.id}"                                           class="mr-3" title="Update Record" data-toggle="tooltip"><span                                                class="fa fa-pencil"></span></a>                                        <a href="${pageContext.request.contextPath}/portfolio/delete/${stock.id}"                                           title="Delete Record" data-toggle="tooltip"><span class="fa fa-trash"></span></a>                                    </td>                                </tr>                            </c:forEach>                            </tbody>                        </table>                    </c:when>                    <c:otherwise>                        <div class="alert alert-danger"><em>No records were found.</em></div>                    </c:otherwise>                </c:choose>            </div>        </div>    </div></div><div class="wrapper-s" style="position:absolute; left:910px">    <div class="container-fluid">        <div class="row">            <div class="col-md-12">                <div class="mt-5 mb-3 clearfix">                    <h2 class="pull-left">Current Details</h2>                    <c:choose>                        <c:when test="${not empty holdings}">                            <table class="table table-bordered table-striped">                                <thead>                                <tr>                                    <th>Stock Name</th>                                    <th>Selling Value</th>                                    <th>Buying Value</th>                                    <th>Holding Value</th>                                    <th>P&L</th>                                    <th>Stock Status</th>                                </tr>                                </thead>                                <tbody>                                <c:forEach var="holding" items="${holdings}">                                    <tr>                                        <td>${holding.stock}</td>                                        <td>${holding.sellValue}</td>                                        <td>${holding.buyValue}</td>                                        <td>${holding.holdValue}</td>                                        <td>                                            <c:choose>                                                <c:when test="${holding.profit eq 0}">                                                    N/A                                                </c:when>                                                <c:otherwise>                                                    ${holding.profit}                                                </c:otherwise>                                            </c:choose>                                        </td>                                        <td>${holding.status}</td>                                    </tr>                                </c:forEach>                                </tbody>                            </table>                        </c:when>                        <c:otherwise>                            <div class="alert alert-danger"><em>No records were found.</em></div>                        </c:otherwise>                    </c:choose>                </div>            </div>        </div>    </div></div><div style="height:800px">    <jsp:include page="../home.jsp"/></div></body></html>