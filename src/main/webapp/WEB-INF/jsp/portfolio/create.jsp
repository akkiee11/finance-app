<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <title>Create Record</title>    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">    <style>      .wrapper {        width: 600px;        margin: 0 auto;      }    </style>    <%-- TODO: Add Form Validation --%>    <!--<script>        function validateForm() {            let form = document.forms["create_stock_entry"];            let err_msg;            let stock_name = form["stock_name"].value.trim();            if (stock_name === "") {                err_msg = "Name must be filled out";            }            let stock_price = form["stock_price"].value;            if (stock_price === "") {                err_msg = "Name must be filled out";            }            let stock_quantity = form["stock_name"].value;            if (stock_name === "") {                err_msg = "Name must be filled out";            }        }    </script>--></head><body><div class="wrapper">    <div class="container-fluid">        <div class="row">            <div class="col-md-12">                <h2 class="mt-5">Create Record</h2>                <p>Please fill this form and submit to add stock record to the database.</p>                <form name="create_stock_entry" action="${pageContext.request.contextPath}/portfolio/create" onclick=""                      method="post">                    <div class="form-group">                        <label>Stock Name</label>                        <label>                            <input class="form-control" name="stock_name" type="text">                        </label>                    </div>                    <div class="form-group">                        <label>Price</label>                        <label>                            <input class="form-control" name="stock_price" type="number">                        </label>                    </div>                    <div class="form-group">                        <label>Quantity</label>                        <label>                            <input class="form-control" name="stock_quantity" type="number">                        </label>                    </div>                    <div class="form-group">                        <label for="current-position">Current Position</label><br>                        <select id="current-position" name="stock_position">                            <option value="BUY">BUY</option>                            <option value="SELL">SELL</option>                        </select>                    </div>                    <input type="submit" class="btn btn-primary" value="Submit">                    <input type="button" class="btn btn-secondary ml-2" value="Cancel" onclick="history.back()">                </form>            </div>        </div>    </div></div></body></html>